<h1>New Playlist Review</h1>
<h2>add a collection select to review a playlist from</h2>
<%= form_for ([@playlist, @review]) do |f| %>
    Select Playlist Title: <%= f.collection_select :playlist, Playlist.all, :id, :title %>
    Select Playlist by: <%= f.collection_select :user, User.all, :id, :username %><br><br>
    <%= f.label :rating %>
    <%= f.number_field :rating%><br><br>

    <%= f.label :comment %>
    <%= f.text_area :comment %><br><br>

    <%= f.submit "Add Review"%>

<% end %>
