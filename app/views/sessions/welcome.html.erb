<h1>Get into the Playlists & Review</h1>


<h3>Welcome, let's <%= link_to 'Log In', '/login' %> or <%= link_to 'Sign Up', '/signup' </h3>

<%#--!
<% if session[:user_id] %>
  <h1><%= @user.username %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>Google UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
<% else %>
    <%= link_to('Log in with Google!', '/auth/google', method: :post) %>
<% end %>
--%>